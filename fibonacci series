n = int(input())
n1 , n2 = 0, 1
count = 0
if n <= 0 :
   print(" ")
elif n == 1 :
     print("fibonacci series", n, ":" )
     print(n1)
else :
     print("fibonacci series :")
     while count < n :
           print(n2)
           nth = n1 + n2
           n1 = n2
           n2 = nth
           count += 1
